{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "google",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "react/no-unescaped-entities": 0,
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": 0,
    "@next/next/no-img-element": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
